MODULE_LICENSE_APACHE2 filter=lfs diff=lfs merge=lfs -text
src filter=lfs diff=lfs merge=lfs -text
android filter=lfs diff=lfs merge=lfs -text
LICENSE filter=lfs diff=lfs merge=lfs -text
METADATA filter=lfs diff=lfs merge=lfs -text
profile.sh filter=lfs diff=lfs merge=lfs -text
README.md filter=lfs diff=lfs merge=lfs -text
